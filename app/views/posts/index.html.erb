<main class="posts-page container">
  <section class="posts">
    <% if @posts.any? %>
      <%= render @posts %>
    <% else %>
      Nenhum Post.
    <% end %>
  </section>

  <aside>
     <%= render current_user %>

    <section class="seggestions">

      <h4>Sugestões Para Você</h4>
      <%= render @suggested_users %>

    </section>
  </aside>

  <footer>
    <%= Date.current.year %> Instagran Clone
  </footer>

</main>